{
    "name": "zainaldeen/sanctum-refresh-token",
    "description": "Provide Refresh token for sanctum",
    "keywords": ["laravel", "sanctum", "auth", "refresh-token"],
    "license": "MIT",
    "support": {
        "issues": "https://github.com/zainaldeen/sanctum-refresh-token/issues",
        "source": "https://github.com/zainaldeen/sanctum-refresh-token"
    },
    "authors": [
        {
            "name": "ZainAldeen Fayod",
            "email": "zainaldeenfayod@gmail.com"
        }
    ],
    "require": {
        "php": "^7.2|^8.0",
        "ext-json": "*",
        "illuminate/contracts": "^6.9|^7.0|^8.0",
        "illuminate/database": "^6.9|^7.0|^8.0",
        "illuminate/support": "^6.9|^7.0|^8.0"
    },
    "require-dev": {
        "mockery/mockery": "^1.0",
        "orchestra/testbench": "^4.0|^5.0|^6.0",
        "phpunit/phpunit": "^8.0|^9.3"
    },
    "autoload": {
        "psr-4": {
            "Zainaldeen\\SanctumRefreshToken\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Zainaldeen\\SanctumRefreshToken\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.x-dev"
        },
        "laravel": {
            "providers": [
                "Zainaldeen\\SanctumRefreshToken\\SanctumServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
